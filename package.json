{
  "name": "steamdashboard",
  "version": "1.0.0",
  "description": "dashboard",
  "main": "index.js",
  "author": "stella heath",
  "license": "ISC",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "webpack-dev-server",
    "start:backend": "./.pyenv/bin/tomb serve",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "npm run build:backend:venv && npm run build:backend:install",
    "build:backend:venv": "python3.5 -m venv .pyenv",
    "build:backend:install": "./.pyenv/bin/pip install -r reqs/install.txt -e .",
    "build:frontend": "npm install"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
